		<parameter type="integer" index="19.7" size="0.1" param="STORM_STATE"/>
		<parameter type="integer" index="20.0" size="0.5" param="WIND_DIRECTION"/>
		<parameter type="integer" index="20.5" size="0.1" param="BATTERY_STATE"/>
		<!-- <parameter type="integer" index="20.6" size="0.1" param="STORM_STATE"/>-->
		<parameter type="integer" index="21.0" size="2.0" param="BRIGHTNESS_LUX"/>
		<parameter type="integer" index="23.0" size="1.0" param="UV_INDEX"/>
		<parameter type="integer" index="24.0" size="1.0" signed="true" param="RSSI"/>
	</frame>
	<frame id="TH_EVENT" direction="from_device" event="true" type="#i" channel_field="10">
		<parameter type="integer" signed="true" index="11.0" size="2.0" param="TH_TEMPERATURE"/>
		<parameter type="integer" index="13.0" size="1.0" param="TH_HUMIDITY"/>
	</frame>
</frames>

