				</logical>
				<physical size="2.0" type="integer" interface="eeprom" endian="little">
					<address index="+10.0"/>
				</physical>
				<conversion type="float_integer_scale" factor="100"/>
				<conversion type="integer_integer_map">
					<value_map to_device="false" from_device="true" parameter_value="2300" device_value="0xffff"/>
				</conversion>
			</parameter>
			<parameter id="ALARM_MAX_POWER">
				<logical type="float" unit="W" default="50.0" min="0.1" max="384.0">
					<special_value id="NOT_USED" value="0"/>
				</logical>
				<physical size="2.0" type="integer" interface="eeprom" endian="little">
					<address index="+12.0"/>
				</physical>
				<conversion type="float_integer_scale" factor="100"/>
				<conversion type="integer_integer_map">
					<value_map to_device="false" from_device="true" parameter_value="500" device_value="0xffff"/>
				</conversion>
			</parameter>
		</paramset>
		
		<paramset type="LINK" id="hmw_pm_key_ch_link" peer_param="ACTUATOR" channel_param="CHANNEL" count="20" address_start="0x2CC" address_step="6">
			<parameter id="CHANNEL" operations="none" hidden="true">
				<logical type="integer" min="0" max="255" default="255"/>
				<physical type="integer" size="1.0" interface="eeprom">
					<address index="+0"/>
				</physical>
			</parameter>
			<parameter id="ACTUATOR" operations="none" hidden="true">
				<logical type="address"/>
				<physical type="array">
					<physical type="integer" size="4.0" interface="eeprom">
						<address index="+1"/>
					</physical>
					<physical type="integer" size="1.0" interface="eeprom">
						<address index="+5"/>
					</physical>
				</physical>
			</parameter>
		</paramset>
		
		<paramset type="VALUES" id="hmw_pm_output_values">
			<parameter id="BUS_VOLTAGE" operations="read,event">
				<logical type="float" min="-273.15" max="327.67" unit="V"/>
				<physical type="integer" interface="command" value_id="BUS_VOLTAGE">
					<get request="LEVEL_GET" response="PM_EVENT"/>
					<event frame="PM_EVENT"/>
				</physical>
				<conversion type="float_integer_scale" factor="100"/>
			</parameter>
			<parameter id="BUS_CURRENT" operations="read,event">
				<logical type="float" min="-273.15" max="327.67" unit="A"/>
				<physical type="integer" interface="command" value_id="BUS_CURRENT">
					<get request="LEVEL_GET" response="PM_EVENT"/>
					<event frame="PM_EVENT"/>
				</physical>
				<conversion type="float_integer_scale" factor="100"/>
			</parameter>
			<parameter id="BUS_POWER" operations="read,event">
				<logical type="float" min="-273.15" max="327.67" unit="W"/>
				<physical type="integer" interface="command" value_id="BUS_POWER">
					<get request="LEVEL_GET" response="PM_EVENT"/>
					<event frame="PM_EVENT"/>
				</physical>
				<conversion type="float_integer_scale" factor="100"/>
			</parameter>
			<parameter id="STATE" operations="read,event">
				<logical type="option">
					<option id="Good" default="true"/>
					<option id="Over Voltage"/>
					<option id="Over Power"/>
					<option id="Overload"/>
					<option id="Under Voltage"/>
					<option id="Error"/>
					<option id="Under Voltage and Over Power"/>
					<option id="Error"/><!-- 7 -->
					<option id="Sensor Fail"/>
					<option id="Sensor Fail"/>
					<option id="Sensor Fail"/>
					<option id="Sensor Fail"/>
					<option id="Sensor Fail"/>
					<option id="Sensor Fail"/>
					<option id="Sensor Fail"/>
					<option id="Error"/>
				</logical>
				<physical type="integer" interface="command" value_id="ALARM_FLAGS">
					<get request="LEVEL_GET" response="PM_EVENT"/>
					<event frame="PM_EVENT"/>
				</physical>
			</parameter>
		</paramset>

	</channel>

