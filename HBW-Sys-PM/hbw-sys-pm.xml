			<parameter id="ENABLED">
				<logical type="boolean" default="true"/>
				<physical size="0.1" type="integer" interface="eeprom">
					<address index="+0"/>
				</physical>
				<conversion type="boolean_integer" invert="false"/>
			</parameter>
			<parameter id="KEY_EVENT_ALARM">
				<logical type="boolean" default="true"/>
				<physical size="0.1" type="integer" interface="eeprom">
					<address index="+0.1"/>
				</physical>
				<conversion type="boolean_integer" invert="true"/>
			</parameter>
			<parameter id="SAMPLE_RATE">
				<logical type="float" default="30.0" min="0.5" max="127.0" unit="s (4 samples)">
					<!-- <special_value id="INPUT_DISABLED" value="0"/> -->
				</logical>
				<physical size="1.0" type="integer" interface="eeprom">
					<address index="+3.0"/>
				</physical>
				<conversion type="float_integer_scale" factor="2"/>
				<conversion type="integer_integer_map">
					<value_map to_device="false" from_device="true" parameter_value="60" device_value="0xff"/>
				</conversion>
			</parameter>
			
			<parameter id="SEND_MIN_INTERVAL">
				<logical type="integer" unit="s" default="60" min="15" max="3600">
					<special_value id="NOT_USED" value="0"/>
				</logical>
				<physical size="2.0" type="integer" interface="eeprom" endian="little">
					<address index="+4.0"/>
				</physical>
				<conversion type="integer_integer_map">
					<value_map to_device="false" from_device="true" parameter_value="60" device_value="0xffff"/>
				</conversion>
			</parameter>
			<parameter id="SEND_MAX_INTERVAL">
				<logical type="integer" unit="s" default="600" min="15" max="3600">
					<special_value id="NOT_USED" value="0"/>
				</logical>
				<physical size="2.0" type="integer" interface="eeprom" endian="little">
					<address index="+6.0"/>
				</physical>
				<conversion type="integer_integer_map">
					<value_map to_device="false" from_device="true" parameter_value="600" device_value="0xffff"/>
				</conversion>
			</parameter>
			<parameter id="ALARM_MAX_VOLTAGE">
				<logical type="float" unit="V" default="25.5" min="0.1" max="48.0">
					<special_value id="NOT_USED" value="0"/>
				</logical>
				<physical size="2.0" type="integer" interface="eeprom" endian="little">
					<address index="+8.0"/>
				</physical>
				<conversion type="float_integer_scale" factor="100"/>
				<conversion type="integer_integer_map">
					<value_map to_device="false" from_device="true" parameter_value="2550" device_value="0xffff"/>
				</conversion>
			</parameter>
			<parameter id="ALARM_MIN_VOLTAGE">
				<logical type="float" unit="V" default="23.0" min="0.1" max="48.0">
